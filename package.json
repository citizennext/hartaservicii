{
  "name": "harta-servicii",
  "version": "0.2.0",
  "author": {
    "Cezar Neaga <cezar.neaga@citizennext.ro>"
    "Stefan Iurasog <si@codesicle.com>"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache && yarn start",
    "lint": "eslint . --ext ts --ext tsx",
    "format": "prettier --config ./.prettierrc --write src/**/*.tsx",
    "start": "gatsby develop",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "npm run build:dir && npm run build:favicons && gatsby build",
    "test": "NODE_ENV=test ava **/*.test.js --verbose",
    "type-check": "tsc --pretty --noEmit --skipLibCheck"
  },
  "greenkeeper": {
    "ignore": [
      "ava"
    ]
  },
  "dependencies": {
    "@openfonts/koho_all": "^1.44.1",
    "gatsby": "^2.18.5",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-catch-links": "^2.1.19",
    "gatsby-plugin-manifest": "^2.2.30",
    "gatsby-plugin-netlify": "^2.1.27",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "^3.0.25",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.4",
    "gatsby-plugin-sitemap": "^2.2.22",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-plugin-typescript": "^2.1.20",
    "gatsby-source-filesystem": "^2.1.39",
    "gatsby-theme-style-guide": "^0.2.49",
    "gatsby-transformer-sharp": "^2.3.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
    "react-helmet": "^5.2.1",
    "react-stickynode": "^2.1.1",
    "typeface-montserrat": "^0.0.75"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/citizennext/hartaservicii"
  },
  "bugs": {
    "url": "https://github.com/citizennext/hartaservicii/issues"
  }
}
