    "husky": "^4.2.1",
    "leaflet": "^1.6.0",
    "leaflet.markercluster": "^1.0.0",
    "lint-staged": "^10.0.3",
    "node-sass": "^4.13.1",
    "rc-drawer": "^3.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
