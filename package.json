    "gatsby-transformer-sharp": "~2.3.9",
    "node-sass": "~4.13.0",
    "rc-drawer": "~3.1.1",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "react-feather": "~2.0.3",
    "react-helmet": "~5.2.1",
    "react-stickynode": "~2.1.1",
    "tailwindcss": "~1.1.4",
    "theme-ui": "~0.2.52",
    "typeface-montserrat": "~0.0.75"
